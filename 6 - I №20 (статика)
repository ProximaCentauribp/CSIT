#include<iostream>

using namespace std;

int main() {
    int n; cin >> n;
    int a[100];
    for (int i = 0; i < n; i++)
        cin >> a[i];
    for (int i = 1; i < n; i += 2)
        if (a[i] > 0) cout << a[i] << ' ';
}
/////////////////////

#include<iostream>

using namespace std;

int main() {
    int n; cin >> n;
    int a[100][100];
    for (int i = 0; i < n; i++)
        for (int j = 0; j < n; j++)
            cin >> a[i][j];
    for (int i = 1; i < n; i += 2)
        for (int j = 0; j < n; j++)
            if (a[i][j] > 0) cout << a[i][j] << ' ';
}
