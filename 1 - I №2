#include <iostream>
#include <cmath>
#include <iomanip> // импортируем манипулятор

using namespace std;

int main()
{
    double x, y;
    cin >> x >> y;
    // использем мунипулятор для форматирования
    // входного потока
    cout << fixed << setprecision(10);
    cout << exp(-x) - cos(x) + sin(2 * x * y);
}
